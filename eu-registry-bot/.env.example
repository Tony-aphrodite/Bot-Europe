# EU Registry Bot - Environment Variables

# Certificate Configuration
CERTIFICATE_PATH=./certificates/certificate.p12
CERTIFICATE_PASSWORD=your_certificate_password

# Portugal Portal
PORTUGAL_PORTAL_URL=https://www.gov.pt
PORTUGAL_AUTH_URL=https://www.autenticacao.gov.pt

# France Portal
FRANCE_PORTAL_URL=https://www.service-public.gouv.fr
FRANCE_FORM_URL=https://entreprendre.service-public.gouv.fr

# Scheduler Settings
SCHEDULER_ENABLED=true
SCHEDULER_HOUR=9
SCHEDULER_MINUTE=0

# Logging
LOG_LEVEL=INFO
LOG_FILE=./logs/bot.log

# Browser Settings
HEADLESS_MODE=true
BROWSER_TIMEOUT=30

# Optional: CAPTCHA Service
# CAPTCHA_API_KEY=your_2captcha_api_key
